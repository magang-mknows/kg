    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "stylelint": "^15.0.0",
    "stylelint-config-standard": "^30.0.1",
    "tailwindcss": "^3.2.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "typescript-language-server": "^3.3.0"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "next build",
      "jest --bail --detectOpenHandles --passWithNoTests",
      "prettier --write --ignore-unknown"
