    "clsx": "^1.2.1",
    "husky": "^8.0.3",
    "next": "^13.2.1",
    "next-auth": "^4.20.1",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-avatar": "^5.0.3",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^3.1.4",
